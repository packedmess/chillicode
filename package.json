{
  "name": "chillicode-new-site",
  "version": "2.0.3",
  "private": true,
  "engines": {
    "node": "14.15.4",
    "npm": "6.14.10"
  },
  "scripts": {
    "dev": "npm-run-all -p app:dev storybook:dev",
    "app:dev": "next dev -p 3000",
    "app:build": "next build & next-sitemap",
    "app:export": "next export",
    "app:start": "node server",
    "app:start-pm2": "pm2 start ecosystem.config.js --env production",
    "storybook:dev": "start-storybook -p 3001 --ci --quiet --static-dir ./public",
    "storybook:build": "build-storybook",
    "prettier:inspect": "prettier --check \"./**/*.{js,jsx,json,css,scss}\"",
    "prettier:fix": "prettier --write \"./**/*.{js,jsx,json,css,scss}\"",
    "eslint:inspect": "eslint \"./**/*.{js,jsx}\"",
    "eslint:fix": "eslint \"./**/*.{js,jsx}\" --fix",
    "stylelint:inspect": "stylelint \"./**/*.{css,scss}\" --syntax scss",
    "stylelint:fix": "stylelint \"./**/*.{css,scss}\" --syntax scss --fix",
    "linters:inspect": "npm-run-all -p prettier:inspect stylelint:inspect eslint:inspect",
    "linters:fix": "npm-run-all -p prettier:fix stylelint:fix eslint:fix",
    "heroku-postbuild": "npm run app:build"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@emotion/react": "^11.1.5",
    "@material-ui/core": "^5.0.0-alpha.25",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.14",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "classnames": "^2.2.6",
    "css-loader": "^5.0.1",
    "csso-webpack-plugin": "^2.0.0-beta.1",
    "ejs": "^3.1.6",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-formidable": "^1.2.0",
    "final-form": "^4.20.1",
    "html-react-parser": "^1.2.4",
    "is-in-browser": "^1.1.3",
    "logger": "0.0.1",
    "mkdirp": "^1.0.4",
    "next": "10.0.5",
    "next-i18next": "^7.0.1",
    "next-sitemap": "^1.6.148",
    "node-sass-json-importer": "^4.3.0",
    "nodemailer": "^6.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.8",
    "react": "^16.14.0",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.14.0",
    "react-final-form": "^6.5.2",
    "react-helmet": "^6.1.0",
    "react-markdown": "^6.0.2",
    "react-redux": "^7.2.2",
    "react-responsive-masonry": "^2.1.1",
    "react-scroll": "^1.8.1",
    "react-svg-loader": "^3.0.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-prettier": "^1.1.2",
